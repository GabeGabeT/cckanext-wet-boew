[main]

dont_bundle = jquery.js jquery.mobile.js

order = theme-base/css/theme-min.css grids/css/util-min.css js/css/pe-ap-min.css theme-base/js/theme-min.js js/settings.js js/pe-ap-min.js

[IE conditional]

lte IE 8 = js/polyfills/html5shiv-min.js grids/css/util-ie-min.css theme-base/css/theme-ie-min.css js/css/pe-ap-ie-min.css
gt IE 8 = theme-base/css/theme-min.css grids/css/util-min.css js/css/pe-ap-min.css
others = theme-base/css/theme-min.css grids/css/util-min.css js/css/pe-ap-min.css


[groups]

wet =
  js/polyfills/html5shiv-min.js 
  grids/css/util-ie-min.css 
  theme-base/css/theme-ie-min.css 
  js/css/pe-ap-ie-min.css
  theme-base/css/theme-min.css
  grids/css/util-min.css
  js/css/pe-ap-min.css
  theme-base/js/theme-min.js
  js/settings.js
  js/pe-ap-min.js 
